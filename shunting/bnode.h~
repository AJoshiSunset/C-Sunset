#include <iostream>
#include <cstring>
#include <vector>

using namespace std;

class bnode: {
 public:
  bnode(char);

  void setNext(bnode*);
  
  bnode* next;

  void setRight(bnode*);
  
  bnode* rightbnode;

  void setLeft(bnode*);

  bnode* leftbnode;

  void setSub(bnode*);

  bnode* subbnode;

  char token;
}
