#include <iostream>
#include <cstring>

using namespace std;

#include "node.h"

node::node(student* stuVal) {
  next = NULL;
  stu = stuVal;
}

node::~node() {
  delete stu;
}

node* getNext() {
  return next;

}

void setNext(node* nextVal) {

  next = nextVal;

}

student* getStudent() {
  return stu;
}
