#include <iostream>
#include <ctype.h>
#include <cstring>

using namespace std;

int main()
{
  char input[81];
  
  cin.get(input, 81);
  cin.get();

  int inputlen = strlen(input);

  for (int i = 0; i < inputlen; i++) {
    if (!isalpha(input[i])) {
      int count = i;
      while (count != inputlen) {
	input[count] = input[count + 1];
	count++;
      }
      i--;
      //inputlen--;
    }
  }

  cout << input << endl;

  /*
  //checks for if the input is a palindrome
  for (int i = 0; i < inputlen / 2; i++) {
    if (input[i] != input[inputlen - i - 1]) {
      cout << "Nope!";
      return 0;
    }
  }
  
  
  cout << "Yup!";
  */

  /*
  cout << input << endl;
  cout << "Length: " << inputlen << endl;
  cout << "Copied: " << str2 << endl;
  */
  
  return 0;
}
